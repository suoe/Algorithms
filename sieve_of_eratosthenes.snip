snippet     sieve_of_eratosthenes
alias       eratosthenes
  vector<int> sieve_of_eratosthenes(int n) {
    vector<int> primes(n);
    whole(iota, primes, 0);
    for (int i = 2; i*i < n; ++i)
      if (primes[i])
        for (int j = i*i; j < n; j+=i)
          primes[j] = 0;
    return primes;
  }
